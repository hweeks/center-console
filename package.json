{
  "name": "center-console",
  "version": "1.0.1",
  "description": "a tool to center console output",
  "main": "dist/index.js",
  "typings": "dist/index.ts",
  "author": "hammie",
  "license": "WTFPL",
  "private": false,
  "scripts": {
    "lint": "eslint --ext .ts ./lib",
    "dev": "ts-node-dev --inspect-brk --respawn ./lib/index.ts",
    "dev:brk": "nodemon -e ts,tsx --exec yarn bbb",
    "bbb": "babel-node --extensions \".js,.ts,.tsx\"",
    "prepublishOnly": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/preset-typescript": "^7.12.7",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.8",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.6",
    "semantic-release": "^17.2.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.11"
  }
}
